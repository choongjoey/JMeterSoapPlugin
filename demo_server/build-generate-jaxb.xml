<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project basedir="." default="generate-jaxb-sources" name="demo_server">

	<property file="build.properties" />
	<target name="generate-jaxb-sources" depends="" description="Generates JAXB classes from XSD schema.">
	    <java classname="com.sun.tools.internal.xjc.XJCFacade">
	            <arg value="-d" />
	            <arg value="${generated-source-folder}"/>
	            <arg value="-p" />
	            <arg value="${calc-package}"/>
	            <arg value="${calc-resource-folder}/WEB-INF/calc.xsd" />
	    </java>
	</target>
</project>
